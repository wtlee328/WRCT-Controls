			ADDRESS_COMBINED LIKE ''%/MA/%'' OR
			ADDRESS_COMBINED LIKE ''%/MI/%'' OR
			ADDRESS_COMBINED LIKE ''%/MN/%'' OR
			ADDRESS_COMBINED LIKE ''%/MS/%'' OR
			ADDRESS_COMBINED LIKE ''%/MO/%'' OR
			ADDRESS_COMBINED LIKE ''%/MT/%'' OR
			ADDRESS_COMBINED LIKE ''%/NE/%'' OR
			ADDRESS_COMBINED LIKE ''%/NV/%'' OR
			ADDRESS_COMBINED LIKE ''%/NH/%'' OR
			ADDRESS_COMBINED LIKE ''%/NJ/%'' OR
			ADDRESS_COMBINED LIKE ''%/NM/%'' OR
			ADDRESS_COMBINED LIKE ''%/NY/%'' OR
			ADDRESS_COMBINED LIKE ''%/NC/%'' OR
			ADDRESS_COMBINED LIKE ''%/ND/%'' OR
			ADDRESS_COMBINED LIKE ''%/OH/%'' OR
