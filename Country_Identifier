	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Namibia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Nauru''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NP[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Nepal''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Netherlands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NC[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''New Caledonia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''New Zealand''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NI[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Nicaragua''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Niger''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Nigeria''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NU[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Niue''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Norfolk Island''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]MP[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Northern Mariana Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]NO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Norway''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]RO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Romania''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]OM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Oman''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PK[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Pakistan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Palau''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Palestine, State of''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Panama''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Papua New Guinea''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PY[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Paraguay''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Peru''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Philippines''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Pitcairn''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Poland''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PT[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Portugal''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Puerto Rico''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]QA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Qatar''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]RE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Réunion''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]RU[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Russian Federation''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]RW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Rwanda''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Barthélemy''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Helena, Ascension and Tristan da''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]KN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Kitts and Nevis''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]LC[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Lucia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]MF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Martin''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]PM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Pierre and Miquelon''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VC[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saint Vincent and the Grenadines''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]WS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Samoa''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''San Marino''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]ST[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sao Tome and Principe''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Saudi Arabia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Senegal''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]RS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Serbia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SC[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Seychelles''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sierra Leone''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Singapore''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SX[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sint Maarten''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SK[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Slovakia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SI[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Slovenia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SB[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Solomon Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Somalia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]ZA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''South Africa''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]GS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''South Georgia and the South Sandwich I''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''South Sudan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]ES[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Spain''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]LK[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sri Lanka''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SD[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sudan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Suriname''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SJ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Svalbard and Jan Mayen''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Sweden''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Switzerland''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]SY[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Syrian Arab Republic''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Taiwan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TJ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tajikistan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tanzania, United Republic of''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Thailand''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Timor-Leste''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Togo''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TK[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tokelau''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tonga''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TT[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Trinidad and Tobago''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tunisia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Turkey''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Turkmenistan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TC[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Turks and Caicos Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TV[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Tuvalu''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]UG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Uganda''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]UA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Ukraine''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''United Arab Emirates''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]GB[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Great Britain''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]US[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''USA''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]UM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''United States Minor Outlying Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]UY[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Uruguay''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]UZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Uzbekistan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VU[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Vanuatu''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Venezuela''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Viet Nam''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Virgin Islands (British)''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]VI[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Virgin Islands (US)''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]WF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Wallis and Futuna''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]EH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Western Sahara''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]YE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Yemen''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]ZM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Zambia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]ZW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Zimbabwe''
	end as P2
	FROM FULL_TABLE
	),

	PT3 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE 
	--Pattern 3: Country Code "/" and wildcards on either side
	WHEN ADDRESS_COMBINED LIKE ''%/AF/%'' THEN ''Afghanistan''
	WHEN ADDRESS_COMBINED LIKE ''%/AX/%'' THEN ''Aland Islands''
	WHEN ADDRESS_COMBINED LIKE ''%/AL/%'' THEN ''Albania''
	WHEN ADDRESS_COMBINED LIKE ''%/DZ/%'' THEN ''Algeria''
	WHEN ADDRESS_COMBINED LIKE ''%/AS/%'' THEN ''American Samoa''
	WHEN ADDRESS_COMBINED LIKE ''%/AD/%'' THEN ''Andorra''
	WHEN ADDRESS_COMBINED LIKE ''%/AO/%'' THEN ''Angola''
	WHEN ADDRESS_COMBINED LIKE ''%/AI/%'' THEN ''Anguilla''
	WHEN ADDRESS_COMBINED LIKE ''%/AQ/%'' THEN ''Antarctica''
	WHEN ADDRESS_COMBINED LIKE ''%/AG/%'' THEN ''Antigua and Barbuda''
	WHEN ADDRESS_COMBINED LIKE ''%/AR/%'' THEN ''Argentina''
	WHEN ADDRESS_COMBINED LIKE ''%/AM/%'' THEN ''Armenia''
	WHEN ADDRESS_COMBINED LIKE ''%/AW/%'' THEN ''Aruba''
	WHEN ADDRESS_COMBINED LIKE ''%/AU/%'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%/AT/%'' THEN ''Austria''
	WHEN ADDRESS_COMBINED LIKE ''%/AZ/%'' THEN ''Azerbaijan''
	WHEN ADDRESS_COMBINED LIKE ''%/BS/%'' THEN ''Bahamas''
	WHEN ADDRESS_COMBINED LIKE ''%/BH/%'' THEN ''Bahrain''
	WHEN ADDRESS_COMBINED LIKE ''%/BD/%'' THEN ''Bangladesh''
	WHEN ADDRESS_COMBINED LIKE ''%/BB/%'' THEN ''Barbados''
	WHEN ADDRESS_COMBINED LIKE ''%/BY/%'' THEN ''Belarus''
	WHEN ADDRESS_COMBINED LIKE ''%/BE/%'' THEN ''Belgium''
	WHEN ADDRESS_COMBINED LIKE ''%/BZ/%'' THEN ''Belize''
	WHEN ADDRESS_COMBINED LIKE ''%/BJ/%'' THEN ''Benin''
	WHEN ADDRESS_COMBINED LIKE ''%/BM/%'' THEN ''Bermuda''
	WHEN ADDRESS_COMBINED LIKE ''%/BT/%'' THEN ''Bhutan''
	WHEN ADDRESS_COMBINED LIKE ''%/BO/%'' THEN ''Bolivia''
	WHEN ADDRESS_COMBINED LIKE ''%/BQ/%'' THEN ''Bonaire, Sint Eustatius and Saba''
