	WHEN ADDRESS_COMBINED LIKE ''%Ontario%'' AND ADDRESS_COMBINED LIKE ''%Canada'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Ottawa%'' AND ADDRESS_COMBINED LIKE ''%[a-z][0-9][a-z] [0-9][a-z][0-9]%'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Ottawa%'' AND ADDRESS_COMBINED LIKE ''%Canada'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%Canada%'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%Canada'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%Quebec%'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%Quebec'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%QC%'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%Montreal%'' AND ADDRESS_COMBINED LIKE ''%QC'' THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''%St. Kitts%'' THEN ''Saint Kitts and Nevis''
	WHEN ADDRESS_COMBINED LIKE ''%United States'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%Dublin%'' THEN ''Ireland''
