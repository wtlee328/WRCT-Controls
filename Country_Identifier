	WHEN ADDRESS_COMBINED LIKE ''% LV'' THEN ''Latvia''
	WHEN ADDRESS_COMBINED LIKE ''% LB'' THEN ''Lebanon''
	WHEN ADDRESS_COMBINED LIKE ''% LS'' THEN ''Lesotho''
	WHEN ADDRESS_COMBINED LIKE ''% LR'' THEN ''Liberia''
	WHEN ADDRESS_COMBINED LIKE ''% LY'' THEN ''Libya''
	WHEN ADDRESS_COMBINED LIKE ''% LI'' THEN ''Liechtenstein''
	WHEN ADDRESS_COMBINED LIKE ''% LT'' THEN ''Lithuania''
	WHEN ADDRESS_COMBINED LIKE ''% LU'' THEN ''Luxembourg''
	WHEN ADDRESS_COMBINED LIKE ''% MO'' THEN ''Macao''
	WHEN ADDRESS_COMBINED LIKE ''% MK'' THEN ''Macedonia, the former Yugoslav Republic of''
	WHEN ADDRESS_COMBINED LIKE ''% MG'' THEN ''Madagascar''
	WHEN ADDRESS_COMBINED LIKE ''% MW'' THEN ''Malawi''
	WHEN ADDRESS_COMBINED LIKE ''% MY'' THEN ''Malaysia''
	WHEN ADDRESS_COMBINED LIKE ''% MV'' THEN ''Maldives''
	WHEN ADDRESS_COMBINED LIKE ''% ML'' THEN ''Mali''
	WHEN ADDRESS_COMBINED LIKE ''% MT'' THEN ''Malta''
	WHEN ADDRESS_COMBINED LIKE ''% MH'' THEN ''Marshall Islands''
	WHEN ADDRESS_COMBINED LIKE ''% MQ'' THEN ''Martinique''
	WHEN ADDRESS_COMBINED LIKE ''% MR'' THEN ''Mauritania''
	WHEN ADDRESS_COMBINED LIKE ''% MU'' THEN ''Mauritius''
	WHEN ADDRESS_COMBINED LIKE ''% YT'' THEN ''Mayotte''
	WHEN ADDRESS_COMBINED LIKE ''% MX'' THEN ''Mexico''
	WHEN ADDRESS_COMBINED LIKE ''% FM'' THEN ''Micronesia''
	WHEN ADDRESS_COMBINED LIKE ''% MD'' THEN ''Moldova, Republic of''
	WHEN ADDRESS_COMBINED LIKE ''% MC'' THEN ''Monaco''
	WHEN ADDRESS_COMBINED LIKE ''% MN'' THEN ''Mongolia''
	WHEN ADDRESS_COMBINED LIKE ''% ME'' THEN ''Montenegro''
	WHEN ADDRESS_COMBINED LIKE ''% MS'' THEN ''Montserrat''
	WHEN ADDRESS_COMBINED LIKE ''% MA'' THEN ''Morocco''
	WHEN ADDRESS_COMBINED LIKE ''% MZ'' THEN ''Mozambique''
	WHEN ADDRESS_COMBINED LIKE ''% MM'' THEN ''Myanmar''
	WHEN ADDRESS_COMBINED LIKE ''% NA'' THEN ''Namibia''
	WHEN ADDRESS_COMBINED LIKE ''% NR'' THEN ''Nauru''
	WHEN ADDRESS_COMBINED LIKE ''% NP'' THEN ''Nepal''
	WHEN ADDRESS_COMBINED LIKE ''% NL'' THEN ''Netherlands''
	WHEN ADDRESS_COMBINED LIKE ''% NC'' THEN ''New Caledonia''
	WHEN ADDRESS_COMBINED LIKE ''% NZ'' THEN ''New Zealand''
	WHEN ADDRESS_COMBINED LIKE ''% NI'' THEN ''Nicaragua''
	WHEN ADDRESS_COMBINED LIKE ''% NE'' THEN ''Niger''
	WHEN ADDRESS_COMBINED LIKE ''% NG'' THEN ''Nigeria''
	WHEN ADDRESS_COMBINED LIKE ''% NU'' THEN ''Niue''
	WHEN ADDRESS_COMBINED LIKE ''% NF'' THEN ''Norfolk Island''
	WHEN ADDRESS_COMBINED LIKE ''% MP'' THEN ''Northern Mariana Islands''
	WHEN ADDRESS_COMBINED LIKE ''% NO'' THEN ''Norway''
	WHEN ADDRESS_COMBINED LIKE ''% RO'' THEN ''Romania''
	WHEN ADDRESS_COMBINED LIKE ''% OM'' THEN ''Oman''
	WHEN ADDRESS_COMBINED LIKE ''% PK'' THEN ''Pakistan''
	WHEN ADDRESS_COMBINED LIKE ''% PW'' THEN ''Palau''
	WHEN ADDRESS_COMBINED LIKE ''% PS'' THEN ''Palestine, State of''
	WHEN ADDRESS_COMBINED LIKE ''% PA'' THEN ''Panama''
	WHEN ADDRESS_COMBINED LIKE ''% PG'' THEN ''Papua New Guinea''
	WHEN ADDRESS_COMBINED LIKE ''% PY'' THEN ''Paraguay''
	WHEN ADDRESS_COMBINED LIKE ''% PE'' THEN ''Peru''
	WHEN ADDRESS_COMBINED LIKE ''% PH'' THEN ''Philippines''
	WHEN ADDRESS_COMBINED LIKE ''% PN'' THEN ''Pitcairn''
	WHEN ADDRESS_COMBINED LIKE ''% PL'' THEN ''Poland''
	WHEN ADDRESS_COMBINED LIKE ''% PT'' THEN ''Portugal''
	WHEN ADDRESS_COMBINED LIKE ''% PR'' THEN ''Puerto Rico''
	WHEN ADDRESS_COMBINED LIKE ''% QA'' THEN ''Qatar''
	WHEN ADDRESS_COMBINED LIKE ''% RE'' THEN ''Réunion''
	WHEN ADDRESS_COMBINED LIKE ''% RU'' THEN ''Russian Federation''
	WHEN ADDRESS_COMBINED LIKE ''% RW'' THEN ''Rwanda''
	WHEN ADDRESS_COMBINED LIKE ''% BL'' THEN ''Saint Barthélemy''
	WHEN ADDRESS_COMBINED LIKE ''% SH'' THEN ''Saint Helena, Ascension and Tristan da Cunha''
	WHEN ADDRESS_COMBINED LIKE ''% KN'' THEN ''Saint Kitts and Nevis''
	WHEN ADDRESS_COMBINED LIKE ''% LC'' THEN ''Saint Lucia''
	WHEN ADDRESS_COMBINED LIKE ''% MF'' THEN ''Saint Martin''
	WHEN ADDRESS_COMBINED LIKE ''% PM'' THEN ''Saint Pierre and Miquelon''
	WHEN ADDRESS_COMBINED LIKE ''% VC'' THEN ''Saint Vincent and the Grenadines''
	WHEN ADDRESS_COMBINED LIKE ''% WS'' THEN ''Samoa''
	WHEN ADDRESS_COMBINED LIKE ''% SM'' THEN ''San Marino''
	WHEN ADDRESS_COMBINED LIKE ''% ST'' THEN ''Sao Tome and Principe''
	WHEN ADDRESS_COMBINED LIKE ''% SA'' THEN ''Saudi Arabia''
	WHEN ADDRESS_COMBINED LIKE ''% SN'' THEN ''Senegal''
	WHEN ADDRESS_COMBINED LIKE ''% RS'' THEN ''Serbia''
	WHEN ADDRESS_COMBINED LIKE ''% SC'' THEN ''Seychelles''
