	WHEN ADDRESS_COMBINED LIKE ''%PARIS%'' THEN ''France''
	WHEN ADDRESS_COMBINED LIKE ''%AUS'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%BRASIL%'' THEN ''Brazil''
	WHEN ADDRESS_COMBINED LIKE ''%KOREA%'' THEN ''South Korea''
	WHEN ADDRESS_COMBINED LIKE ''%SAINT LUCIA%'' THEN ''Saint Lucia''
	WHEN ADDRESS_COMBINED LIKE ''%ST. LUCIA%'' THEN ''Saint Lucia''
	WHEN ADDRESS_COMBINED LIKE ''%LIPA CITY%'' THEN ''Philippines''
	WHEN ADDRESS_COMBINED LIKE ''%STA.ANASTACIA SANTO TOMAS%'' THEN ''Philippines''
	WHEN ADDRESS_COMBINED LIKE ''%Punta Calamba'' THEN ''Philippines''
	WHEN ADDRESS_COMBINED LIKE ''%UAE'' THEN ''United Arab Emirates''
	WHEN ADDRESS_COMBINED LIKE ''%UAE%'' THEN ''United Arab Emirates''
	WHEN ADDRESS_COMBINED LIKE ''%U.A.E.'' THEN ''United Arab Emirates''
