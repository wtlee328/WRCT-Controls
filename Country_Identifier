	WHEN ADDRESS_COMBINED LIKE ''%Salzburg%'' THEN ''Austria''
	WHEN ADDRESS_COMBINED LIKE ''%New South Wales'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%New South Wales%'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''% NSW%'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%AU [0-9][0-9][0-9][0-9]'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''% VIC %'' AND ADDRESS_COMBINED LIKE ''%[0-9][0-9][0-9][0-9]%'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%VG[0-9][0-9][0-9][0-9]'' THEN '' Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%Tortola%'' THEN '' Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%Virgin Islands%'' AND ADDRESS_COMBINED LIKE ''%British'' THEN '' Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%Virgin Islands%'' AND ADDRESS_COMBINED LIKE ''%British%'' THEN '' Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%BVI'' THEN '' Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%PR [0-9][0-9][0-9][0-9][0-9]'' THEN ''Puerto Rico''
	WHEN ADDRESS_COMBINED LIKE ''%GB%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%London%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9][a-z] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Edinburgh%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Edinburgh%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Edinburg%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Edinburg%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Wokingham%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Berkshire%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Devon%'' AND ADDRESS_COMBINED LIKE ''%[a-z][a-z][0-9] [0-9][a-z][a-z]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%West Yorkshire [a-z][a-z][0-9]%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''% UK'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%Istanbul'' THEN ''Turkey''
	WHEN ADDRESS_COMBINED LIKE ''%Istanbul%'' THEN ''Turkey''
	WHEN ADDRESS_COMBINED LIKE ''%Kuala Lumpur'' THEN ''Malaysia''
	WHEN ADDRESS_COMBINED LIKE ''%Kuala Lumpur%'' THEN ''Malaysia''
	WHEN ADDRESS_COMBINED LIKE ''%Russia'' THEN ''Russian Federation''
	WHEN ADDRESS_COMBINED LIKE ''%Budapest'' THEN ''Hungary''
	WHEN ADDRESS_COMBINED LIKE ''%Budapest%'' THEN ''Hungary''
	WHEN ADDRESS_COMBINED LIKE ''% DK-[0-9][0-9][0-9][0-9]%'' THEN ''Denmark''
	WHEN ADDRESS_COMBINED LIKE ''% Slovenija'' THEN ''Slovenia''
	WHEN ADDRESS_COMBINED LIKE ''% Italia'' THEN ''Italy''
	WHEN ADDRESS_COMBINED LIKE ''%Italia'' THEN ''Italy''
	WHEN ADDRESS_COMBINED LIKE ''% Bergamo'' THEN ''Italy''
	WHEN ADDRESS_COMBINED LIKE ''% (IE)'' THEN ''Ireland''
	end as P9
	FROM FULL_TABLE
	),

	PT10 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 11: Country code with "/" only on the right side and wildcards on both sides
	WHEN ADDRESS_COMBINED LIKE ''% AF/%'' THEN ''Afghanistan''
