	WHEN ADDRESS_COMBINED LIKE ''%/ZW'' THEN ''Zimbabwe''
	end as P8
	FROM FULL_TABLE
	),

	PT9 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 10: Additional exceptions
	WHEN ADDRESS_COMBINED LIKE ''%United States'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%States%'' and ADDRESS_COMBINED LIKE ''%[0-9][0-9][0-9][0-9][0-9]%''THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% US'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%US-&'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% US %'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%Los Angeles%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%Albany%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%Chiyoda%'' THEN ''Japan''
	WHEN ADDRESS_COMBINED LIKE ''%JPN'' THEN ''Japan''
	WHEN ADDRESS_COMBINED LIKE ''%CHN'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Fujian%'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Chaoyang District'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Chaoyang District%'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%JingJiang Province'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%JingJiang Province%'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Zhejiang%'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Ningbo%'' THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''%Macau%'' THEN ''Macau''
	WHEN ADDRESS_COMBINED LIKE ''%Taipei%'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''%Hsinchu'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''%Hsinchu%'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''%Hsin-chu'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''%Hsin-chu%'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''%H.K.%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%H.K.'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%HK'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''% HK-%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Canton Road'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Canton Road%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Kowloon'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Kowloon%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Mongkok'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Mongkok%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%tsimshatsui%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%tsimshatsui'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%tsim sha tsui'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%tsim sha tsui%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Kwun Tong'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Kwun Tong%'' THEN ''Hong Kong''
	WHEN ADDRESS_COMBINED LIKE ''%Kwai Chung'' THEN ''Hong Kong''
