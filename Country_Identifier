	WHEN ADDRESS_COMBINED LIKE ''%Harmon Industrial Park%'' THEN ''Guam''
	WHEN ADDRESS_COMBINED LIKE ''%Warzaw'' THEN ''Poland''
	WHEN ADDRESS_COMBINED LIKE ''%Warzaw%'' THEN ''Poland''
	WHEN ADDRESS_COMBINED LIKE ''%Warszawa'' THEN ''Poland''
	WHEN ADDRESS_COMBINED LIKE ''%Warszawa%'' THEN ''Poland''
