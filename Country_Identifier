USE [WRCT]
GO
/****** Object:  StoredProcedure [dbo].[Country_Identifier]    Script Date: 6/3/2019 12:52:56 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

ALTER PROCEDURE [dbo].[Country_Identifier]
@ACCT_NO			VARCHAR(MAX),
@NAME				VARCHAR(MAX),
@ADDRESS 			VARCHAR(MAX),
@Control_Temp_Table	VARCHAR(MAX),
@Country			VARCHAR(MAX)
AS

BEGIN

IF OBJECT_ID('dbo.FULL_TABLE', 'U') IS NOT NULL 
DROP TABLE dbo.FULL_TABLE;

DECLARE @Script VARCHAR(MAX)
SET @Script =	
	
'
   IF OBJECT_ID(''dbo.'+@Control_Temp_Table+''', ''U'') IS NOT NULL 
   DROP TABLE dbo.'+@Control_Temp_Table+';

   SELECT [Transaction Number], '+@ACCT_NO+', '+@NAME+' AS PARTY_NAME, '+@ADDRESS+' as ADDRESS_COMBINED
	INTO FULL_TABLE FROM [dbo].[eGifts_Concat_Table];
	
	WITH PT1 AS(
	SELECT *, CASE
	--Pattern 1: BIC Code 4+2+5=11 digits
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Afghanistan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AX[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Aland Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AL[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Albania''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Algeria''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AS[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''American Samoa''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Andorra''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Angola''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Anguilla''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AQ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Antarctica''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Antigua and Barbuda''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Argentina''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Armenia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Aruba''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AU[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AT[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Austria''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Azerbaijan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BS[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bahamas''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bahrain''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bangladesh''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BB[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Barbados''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BY[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Belarus''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Belgium''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Belize''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BJ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Benin''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bermuda''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BT[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bhutan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bolivia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BQ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bonaire, Sint Eustatius and Saba''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BA[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bosnia and Herzegovina''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Botswana''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BV[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bouvet Island''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Brazil''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]IO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''British Indian Ocean Territory''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BN[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Brunei Darussalam''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Bulgaria''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Burkina Faso''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]BI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Burundi''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CV[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cabo Verde''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]KH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cambodia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cameroon''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CA[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Canada''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]KY[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cayman Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Central African Republic''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Chad''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CL[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Chile''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CN[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''China''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CX[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Christmas Island''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CC[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cocos Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Colombia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]KM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Comoros''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Congo''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Congo, Democratic Republic of the''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cook Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Costa Rica''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Côte d''''Ivoire''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]HR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Croatia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CU[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cuba''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Curaçao''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CY[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Cyprus''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Czech''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Denmark''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DJ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Djibouti''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Dominica''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Dominican Republic''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]EC[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Ecuador''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]EG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Egypt''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SV[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''El Salvador''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GQ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Equatorial Guinea''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ER[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Eritrea''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]EE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Estonia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Eswatini''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ET[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Ethiopia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]FK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Falkland Islands Malvinas''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]FO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Faroe Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]FJ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Fiji''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]FI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Finland''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]FR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''France''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''French Guiana''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]PF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''French Polynesia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''French Southern Territories''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GA[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Gabon''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Gambia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Georgia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]DE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Germany''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Ghana''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Gibraltar''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Greece''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GL[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Greenland''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Grenada''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GP[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guadeloupe''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GU[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guam''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GT[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guatemala''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guernsey''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GN[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guinea''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Guinea-Bissau''
