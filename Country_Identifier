	WHEN ADDRESS_COMBINED LIKE ''%/TL/%'' THEN ''Timor-Leste''
	WHEN ADDRESS_COMBINED LIKE ''%/TG/%'' THEN ''Togo''
	WHEN ADDRESS_COMBINED LIKE ''%/TK/%'' THEN ''Tokelau''
	WHEN ADDRESS_COMBINED LIKE ''%/TO/%'' THEN ''Tonga''
	WHEN ADDRESS_COMBINED LIKE ''%/TT/%'' THEN ''Trinidad and Tobago''
	WHEN ADDRESS_COMBINED LIKE ''%/TN/%'' THEN ''Tunisia''
	WHEN ADDRESS_COMBINED LIKE ''%/TR/%'' THEN ''Turkey''
	WHEN ADDRESS_COMBINED LIKE ''%/TM/%'' THEN ''Turkmenistan''
	WHEN ADDRESS_COMBINED LIKE ''%/TC/%'' THEN ''Turks and Caicos Islands''
	WHEN ADDRESS_COMBINED LIKE ''%/TV/%'' THEN ''Tuvalu''
	WHEN ADDRESS_COMBINED LIKE ''%/UG/%'' THEN ''Uganda''
	WHEN ADDRESS_COMBINED LIKE ''%/UA/%'' THEN ''Ukraine''
	WHEN ADDRESS_COMBINED LIKE ''%/AE/%'' THEN ''United Arab Emirates''
	WHEN ADDRESS_COMBINED LIKE ''%/GB/%'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''%/US/%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''%/UM/%'' THEN ''United States Minor Outlying Islands''
	WHEN ADDRESS_COMBINED LIKE ''%/UY/%'' THEN ''Uruguay''
	WHEN ADDRESS_COMBINED LIKE ''%/UZ/%'' THEN ''Uzbekistan''
	WHEN ADDRESS_COMBINED LIKE ''%/VU/%'' THEN ''Vanuatu''
	WHEN ADDRESS_COMBINED LIKE ''%/VE/%'' THEN ''Venezuela''
	WHEN ADDRESS_COMBINED LIKE ''%/VN/%'' THEN ''Viet Nam''
	WHEN ADDRESS_COMBINED LIKE ''%/VG/%'' THEN ''Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''%/VI/%'' THEN ''Virgin Islands (US)''
	WHEN ADDRESS_COMBINED LIKE ''%/WF/%'' THEN ''Wallis and Futuna''
	WHEN ADDRESS_COMBINED LIKE ''%/EH/%'' THEN ''Western Sahara''
	WHEN ADDRESS_COMBINED LIKE ''%/YE/%'' THEN ''Yemen''
	WHEN ADDRESS_COMBINED LIKE ''%/ZM/%'' THEN ''Zambia''
	WHEN ADDRESS_COMBINED LIKE ''%/ZW/%'' THEN ''Zimbabwe''
	end as P3
	FROM FULL_TABLE
	),

	PT4 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 4: State and zip code combination
	WHEN ADDRESS_COMBINED LIKE ''% AL [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AK [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AZ [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AR [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CO [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CT [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% DE [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% DC [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% FL [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% GA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% HI [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ID [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IL [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IN [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% KS [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% KY [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% LA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ME [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MD [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MI [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MN [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MS [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MO [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MT [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NE [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NV [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NH [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NJ [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NM [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NY [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NC [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ND [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OH [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OK [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OR [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% PA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% RI [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% SC [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% SD [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% TN [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% TX [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% UT [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% VT [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% VA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WA [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WV [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WI [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WY [0-9][0-9][0-9][0-9][0-9]%''  THEN ''USA''
	end as P4
	FROM FULL_TABLE
	),

	PT5 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 5: State and zip code combination with double spaces between	  
	WHEN ADDRESS_COMBINED LIKE ''% AL  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AK  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AZ  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% AR  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CO  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% CT  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% DE  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% DC  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% FL  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% GA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% HI  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ID  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IL  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IN  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% IA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% KS  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% KY  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% LA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ME  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MD  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MI  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MN  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MS  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MO  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% MT  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NE  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NV  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NH  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NJ  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NM  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NY  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% NC  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% ND  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OH  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OK  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% OR  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% PA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% RI  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% SC  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% SD  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% TN  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% TX  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% UT  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% VT  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% VA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WA  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WV  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WI  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% WY  [0-9][0-9][0-9][0-9][0-9]%'' THEN ''USA''

	end as P5
	FROM FULL_TABLE
	),

	PT6 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 6: Identifies all the US addresses and groups them as "USA", State code with comma
	WHEN ADDRESS_COMBINED LIKE ''%[0-9][0-9][0-9][0-9][0-9]%'' AND
			(ADDRESS_COMBINED LIKE ''% AL %'' OR
			ADDRESS_COMBINED LIKE ''% AK %'' OR
			ADDRESS_COMBINED LIKE ''% AZ %'' OR
			ADDRESS_COMBINED LIKE ''% AR %'' OR
			ADDRESS_COMBINED LIKE ''% CA %'' OR
			ADDRESS_COMBINED LIKE ''% CO %'' OR
			ADDRESS_COMBINED LIKE ''% CT %'' OR
			ADDRESS_COMBINED LIKE ''% DE %'' OR
			ADDRESS_COMBINED LIKE ''% DC %'' OR
			ADDRESS_COMBINED LIKE ''% FL %'' OR
			ADDRESS_COMBINED LIKE ''% GA %'' OR
			ADDRESS_COMBINED LIKE ''% HI %'' OR
			ADDRESS_COMBINED LIKE ''% ID %'' OR
			ADDRESS_COMBINED LIKE ''% IL %'' OR
			ADDRESS_COMBINED LIKE ''% IN %'' OR
			ADDRESS_COMBINED LIKE ''% IA %'' OR
			ADDRESS_COMBINED LIKE ''% KS %'' OR
			ADDRESS_COMBINED LIKE ''% KY %'' OR
			ADDRESS_COMBINED LIKE ''% LA %'' OR
			ADDRESS_COMBINED LIKE ''% ME %'' OR
			ADDRESS_COMBINED LIKE ''% MD %'' OR
			ADDRESS_COMBINED LIKE ''% MA %'' OR
			ADDRESS_COMBINED LIKE ''% MI %'' OR
			ADDRESS_COMBINED LIKE ''% MN %'' OR
			ADDRESS_COMBINED LIKE ''% MS %'' OR
			ADDRESS_COMBINED LIKE ''% MO %'' OR
			ADDRESS_COMBINED LIKE ''% MT %'' OR
			ADDRESS_COMBINED LIKE ''% NE %'' OR
			ADDRESS_COMBINED LIKE ''% NV %'' OR
			ADDRESS_COMBINED LIKE ''% NH %'' OR
			ADDRESS_COMBINED LIKE ''% NJ %'' OR
			ADDRESS_COMBINED LIKE ''% NM %'' OR
			ADDRESS_COMBINED LIKE ''% NY %'' OR
			ADDRESS_COMBINED LIKE ''% NC %'' OR
			ADDRESS_COMBINED LIKE ''% ND %'' OR
			ADDRESS_COMBINED LIKE ''% OH %'' OR
			ADDRESS_COMBINED LIKE ''% OK %'' OR
			ADDRESS_COMBINED LIKE ''% OR %'' OR
			ADDRESS_COMBINED LIKE ''% PA %'' OR
			ADDRESS_COMBINED LIKE ''% RI %'' OR
			ADDRESS_COMBINED LIKE ''% SC %'' OR
			ADDRESS_COMBINED LIKE ''% SD %'' OR
			ADDRESS_COMBINED LIKE ''% TN %'' OR
			ADDRESS_COMBINED LIKE ''% TX %'' OR
			ADDRESS_COMBINED LIKE ''% UT %'' OR
			ADDRESS_COMBINED LIKE ''% VT %'' OR
			ADDRESS_COMBINED LIKE ''% VA %'' OR
			ADDRESS_COMBINED LIKE ''% WA %'' OR
			ADDRESS_COMBINED LIKE ''% WV %'' OR
			ADDRESS_COMBINED LIKE ''% WI %'' OR
			ADDRESS_COMBINED LIKE ''% WY %'' OR

			ADDRESS_COMBINED LIKE ''%/AL/%'' OR
			ADDRESS_COMBINED LIKE ''%/AK/%'' OR
			ADDRESS_COMBINED LIKE ''%/AZ/%'' OR
			ADDRESS_COMBINED LIKE ''%/AR/%'' OR
			ADDRESS_COMBINED LIKE ''%/CA/%'' OR
			ADDRESS_COMBINED LIKE ''%/CO/%'' OR
			ADDRESS_COMBINED LIKE ''%/CT/%'' OR
			ADDRESS_COMBINED LIKE ''%/DE/%'' OR
			ADDRESS_COMBINED LIKE ''%/DC/%'' OR
			ADDRESS_COMBINED LIKE ''%/FL/%'' OR
			ADDRESS_COMBINED LIKE ''%/GA/%'' OR
			ADDRESS_COMBINED LIKE ''%/HI/%'' OR
			ADDRESS_COMBINED LIKE ''%/ID/%'' OR
			ADDRESS_COMBINED LIKE ''%/IL/%'' OR
			ADDRESS_COMBINED LIKE ''%/IN/%'' OR
			ADDRESS_COMBINED LIKE ''%/IA/%'' OR
