			ADDRESS_COMBINED LIKE ''%New Mexico%'' OR
			ADDRESS_COMBINED LIKE ''%New York%'' OR
			ADDRESS_COMBINED LIKE ''%North Carolina%'' OR
			ADDRESS_COMBINED LIKE ''%North Dakota%'' OR
			ADDRESS_COMBINED LIKE ''%Ohio%'' OR
			ADDRESS_COMBINED LIKE ''%Oklahoma%'' OR
			ADDRESS_COMBINED LIKE ''%Oregon%'' OR
			ADDRESS_COMBINED LIKE ''%Pennsylvania%'' OR
			ADDRESS_COMBINED LIKE ''%Rhode Island%'' OR
			ADDRESS_COMBINED LIKE ''%South Carolina%'' OR
			ADDRESS_COMBINED LIKE ''%South Dakota%'' OR
			ADDRESS_COMBINED LIKE ''%Tennessee%'' OR
			ADDRESS_COMBINED LIKE ''%Texas%'' OR
			ADDRESS_COMBINED LIKE ''%Utah%'' OR
			ADDRESS_COMBINED LIKE ''%Vermont%'' OR
			ADDRESS_COMBINED LIKE ''%Virginia%'' OR
			ADDRESS_COMBINED LIKE ''%Washington%'' OR
			ADDRESS_COMBINED LIKE ''%West Virginia%'' OR
			ADDRESS_COMBINED LIKE ''%Wisconsin%'' OR
			ADDRESS_COMBINED LIKE ''%Wyoming%'' OR
			
			ADDRESS_COMBINED LIKE ''%ALABAMA'' OR
			ADDRESS_COMBINED LIKE ''%ALASKA'' OR
			ADDRESS_COMBINED LIKE ''%ARIZONA'' OR
			ADDRESS_COMBINED LIKE ''%ARKANSAS'' OR
			ADDRESS_COMBINED LIKE ''%CALIFORNIA'' OR
			ADDRESS_COMBINED LIKE ''%COLORADO'' OR
			ADDRESS_COMBINED LIKE ''%CONNECTICUT'' OR
			ADDRESS_COMBINED LIKE ''%DELAWARE'' OR
			ADDRESS_COMBINED LIKE ''%D.C.'' OR
			ADDRESS_COMBINED LIKE ''%FLORIDA'' OR
			ADDRESS_COMBINED LIKE ''%GEORGIA'' OR
			ADDRESS_COMBINED LIKE ''%HAWAII'' OR
			ADDRESS_COMBINED LIKE ''%IDAHO'' OR
			ADDRESS_COMBINED LIKE ''%ILLINOIS'' OR
			ADDRESS_COMBINED LIKE ''%INDIANA'' OR
			ADDRESS_COMBINED LIKE ''%IOWA'' OR
			ADDRESS_COMBINED LIKE ''%KANSAS'' OR
			ADDRESS_COMBINED LIKE ''%KENTUCKY'' OR
			ADDRESS_COMBINED LIKE ''%LOUISIANA'' OR
			ADDRESS_COMBINED LIKE ''%MAINE'' OR
			ADDRESS_COMBINED LIKE ''%MARYLAND'' OR
			ADDRESS_COMBINED LIKE ''%MASSACHUSETTS'' OR
			ADDRESS_COMBINED LIKE ''%MICHIGAN'' OR
			ADDRESS_COMBINED LIKE ''%MINNESOTA'' OR
			ADDRESS_COMBINED LIKE ''%MISSISSIPPI'' OR
			ADDRESS_COMBINED LIKE ''%MISSOURI'' OR
			ADDRESS_COMBINED LIKE ''%MONTANA'' OR
			ADDRESS_COMBINED LIKE ''%NEBRASKA'' OR
			ADDRESS_COMBINED LIKE ''%NEVADA'' OR
			ADDRESS_COMBINED LIKE ''%NEW HAMPSHIRE'' OR
			ADDRESS_COMBINED LIKE ''%NEW JERSEY'' OR
			ADDRESS_COMBINED LIKE ''%NEW MEXICO'' OR
			ADDRESS_COMBINED LIKE ''%NEW YORK'' OR
			ADDRESS_COMBINED LIKE ''%NORTH CAROLINA'' OR
			ADDRESS_COMBINED LIKE ''%NORTH DAKOTA'' OR
			ADDRESS_COMBINED LIKE ''%OHIO'' OR
			ADDRESS_COMBINED LIKE ''%OKLAHOMA'' OR
			ADDRESS_COMBINED LIKE ''%OREGON'' OR
			ADDRESS_COMBINED LIKE ''%PENNSYLVANIA'' OR
			ADDRESS_COMBINED LIKE ''%RHODE ISLAND'' OR
			ADDRESS_COMBINED LIKE ''%SOUTH CAROLINA'' OR
			ADDRESS_COMBINED LIKE ''%SOUTH DAKOTA'' OR
			ADDRESS_COMBINED LIKE ''%TENNESSEE'' OR
			ADDRESS_COMBINED LIKE ''%TEXAS'' OR
			ADDRESS_COMBINED LIKE ''%UTAH'' OR
			ADDRESS_COMBINED LIKE ''%VERMONT'' OR
			ADDRESS_COMBINED LIKE ''%VIRGINIA'' OR
			ADDRESS_COMBINED LIKE ''%WASHINGTON'' OR
			ADDRESS_COMBINED LIKE ''%WEST VIRGINIA'' OR
			ADDRESS_COMBINED LIKE ''%WISCONSIN'' OR
			ADDRESS_COMBINED LIKE ''%WYOMING'' OR
			ADDRESS_COMBINED LIKE ''%ALABAMA'' OR
			ADDRESS_COMBINED LIKE ''%ALASKA'' OR
			ADDRESS_COMBINED LIKE ''%ARIZONA'' OR
			ADDRESS_COMBINED LIKE ''%ARKANSAS'' OR
			ADDRESS_COMBINED LIKE ''%CALIFORNIA'' OR
			ADDRESS_COMBINED LIKE ''%COLORADO'' OR
			ADDRESS_COMBINED LIKE ''%CONNECTICUT'' OR
			ADDRESS_COMBINED LIKE ''%DELAWARE'' OR
			ADDRESS_COMBINED LIKE ''%D.C.'' OR
			ADDRESS_COMBINED LIKE ''%FLORIDA'' OR
			ADDRESS_COMBINED LIKE ''%GEORGIA'' OR
			ADDRESS_COMBINED LIKE ''%HAWAII'' OR
			ADDRESS_COMBINED LIKE ''%IDAHO'' OR
			ADDRESS_COMBINED LIKE ''%ILLINOIS'' OR
			ADDRESS_COMBINED LIKE ''%INDIANA'' OR
			ADDRESS_COMBINED LIKE ''%IOWA'' OR
			ADDRESS_COMBINED LIKE ''%KANSAS'' OR
			ADDRESS_COMBINED LIKE ''%KENTUCKY'' OR
			ADDRESS_COMBINED LIKE ''%LOUISIANA'' OR
			ADDRESS_COMBINED LIKE ''%MAINE'' OR
			ADDRESS_COMBINED LIKE ''%MARYLAND'' OR
			ADDRESS_COMBINED LIKE ''%MASSACHUSETTS'' OR
			ADDRESS_COMBINED LIKE ''%MICHIGAN'' OR
			ADDRESS_COMBINED LIKE ''%MINNESOTA'' OR
			ADDRESS_COMBINED LIKE ''%MISSISSIPPI'' OR
			ADDRESS_COMBINED LIKE ''%MISSOURI'' OR
			ADDRESS_COMBINED LIKE ''%MONTANA'' OR
			ADDRESS_COMBINED LIKE ''%NEBRASKA'' OR
			ADDRESS_COMBINED LIKE ''%NEVADA'' OR
			ADDRESS_COMBINED LIKE ''%NEW HAMPSHIRE'' OR
			ADDRESS_COMBINED LIKE ''%NEW JERSEY'' OR
			ADDRESS_COMBINED LIKE ''%NEW MEXICO'' OR
			ADDRESS_COMBINED LIKE ''%NEW YORK'' OR
			ADDRESS_COMBINED LIKE ''%NORTH CAROLINA'' OR
			ADDRESS_COMBINED LIKE ''%NORTH DAKOTA'' OR
			ADDRESS_COMBINED LIKE ''%OHIO'' OR
			ADDRESS_COMBINED LIKE ''%OKLAHOMA'' OR
			ADDRESS_COMBINED LIKE ''%OREGON'' OR
			ADDRESS_COMBINED LIKE ''%PENNSYLVANIA'' OR
			ADDRESS_COMBINED LIKE ''%RHODE ISLAND'' OR
			ADDRESS_COMBINED LIKE ''%SOUTH CAROLINA'' OR
			ADDRESS_COMBINED LIKE ''%SOUTH DAKOTA'' OR
			ADDRESS_COMBINED LIKE ''%TENNESSEE'' OR
			ADDRESS_COMBINED LIKE ''%TEXAS'' OR
			ADDRESS_COMBINED LIKE ''%UTAH'' OR
			ADDRESS_COMBINED LIKE ''%VERMONT'' OR
			ADDRESS_COMBINED LIKE ''%VIRGINIA'' OR
			ADDRESS_COMBINED LIKE ''%WASHINGTON'' OR
			ADDRESS_COMBINED LIKE ''%WEST VIRGINIA'' OR
			ADDRESS_COMBINED LIKE ''%WISCONSIN'' OR
			ADDRESS_COMBINED LIKE ''%WYOMING''
 			)THEN ''USA''

	end as P6
	FROM FULL_TABLE
	),
	
	PT7 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 8: Wildcard comma 2-digit Country Code (at end of address)
	WHEN ADDRESS_COMBINED LIKE ''%,AF'' THEN ''Afghanistan''
	WHEN ADDRESS_COMBINED LIKE ''%,AX'' THEN ''Aland Islands''
	WHEN ADDRESS_COMBINED LIKE ''%,AL'' THEN ''Albania''
	WHEN ADDRESS_COMBINED LIKE ''%,DZ'' THEN ''Algeria''
	WHEN ADDRESS_COMBINED LIKE ''%,AS'' THEN ''American Samoa''
	WHEN ADDRESS_COMBINED LIKE ''%,AD'' THEN ''Andorra''
	WHEN ADDRESS_COMBINED LIKE ''%,AO'' THEN ''Angola''
	WHEN ADDRESS_COMBINED LIKE ''%,AI'' THEN ''Anguilla''
	WHEN ADDRESS_COMBINED LIKE ''%,AQ'' THEN ''Antarctica''
	WHEN ADDRESS_COMBINED LIKE ''%,AG'' THEN ''Antigua and Barbuda''
	WHEN ADDRESS_COMBINED LIKE ''%,AR'' THEN ''Argentina''
	WHEN ADDRESS_COMBINED LIKE ''%,AM'' THEN ''Armenia''
	WHEN ADDRESS_COMBINED LIKE ''%,AW'' THEN ''Aruba''
	WHEN ADDRESS_COMBINED LIKE ''%,AU'' THEN ''Australia''
	WHEN ADDRESS_COMBINED LIKE ''%,AT'' THEN ''Austria''
	WHEN ADDRESS_COMBINED LIKE ''%,AZ'' THEN ''Azerbaijan''
	WHEN ADDRESS_COMBINED LIKE ''%,BS'' THEN ''Bahamas''
	WHEN ADDRESS_COMBINED LIKE ''%,BH'' THEN ''Bahrain''
	WHEN ADDRESS_COMBINED LIKE ''%,BD'' THEN ''Bangladesh''
	WHEN ADDRESS_COMBINED LIKE ''%,BB'' THEN ''Barbados''
	WHEN ADDRESS_COMBINED LIKE ''%,BY'' THEN ''Belarus''
	WHEN ADDRESS_COMBINED LIKE ''%,BE'' THEN ''Belgium''
	WHEN ADDRESS_COMBINED LIKE ''%,BZ'' THEN ''Belize''
	WHEN ADDRESS_COMBINED LIKE ''%,BJ'' THEN ''Benin''
	WHEN ADDRESS_COMBINED LIKE ''%,BM'' THEN ''Bermuda''
	WHEN ADDRESS_COMBINED LIKE ''%,BT'' THEN ''Bhutan''
	WHEN ADDRESS_COMBINED LIKE ''%,BO'' THEN ''Bolivia''
