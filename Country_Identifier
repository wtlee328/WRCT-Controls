	WHEN ADDRESS_COMBINED LIKE ''% ZM,%'' THEN ''Zambia''
	WHEN ADDRESS_COMBINED LIKE ''% ZW,%'' THEN ''Zimbabwe''
	end as P11
	FROM FULL_TABLE
	),

	PT12 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 13: Country with wildcard only on left; for the ending of the address
	WHEN ADDRESS_COMBINED LIKE ''%AFGHANISTAN'' THEN ''Afghanistan''
	WHEN ADDRESS_COMBINED LIKE ''%ALAND ISLANDS'' THEN ''Aland Islands''
	WHEN ADDRESS_COMBINED LIKE ''%ALBANIA'' THEN ''Albania''
	WHEN ADDRESS_COMBINED LIKE ''%ALGERIA'' THEN ''Algeria''
