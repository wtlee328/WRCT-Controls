	WHEN ADDRESS_COMBINED LIKE ''% SO'' THEN ''Somalia''
	WHEN ADDRESS_COMBINED LIKE ''% ZA'' THEN ''South Africa''
	WHEN ADDRESS_COMBINED LIKE ''% GS'' THEN ''South Georgia and the South Sandwich Islands''
	WHEN ADDRESS_COMBINED LIKE ''% SS'' THEN ''South Sudan''
	WHEN ADDRESS_COMBINED LIKE ''% ES'' THEN ''Spain''
	WHEN ADDRESS_COMBINED LIKE ''% LK'' THEN ''Sri Lanka''
	WHEN ADDRESS_COMBINED LIKE ''% SD'' THEN ''Sudan''
	WHEN ADDRESS_COMBINED LIKE ''% SR'' THEN ''Suriname''
	WHEN ADDRESS_COMBINED LIKE ''% SJ'' THEN ''Svalbard and Jan Mayen''
	WHEN ADDRESS_COMBINED LIKE ''% SE'' THEN ''Sweden''
	WHEN ADDRESS_COMBINED LIKE ''% CH'' THEN ''Switzerland''
	WHEN ADDRESS_COMBINED LIKE ''% SY'' THEN ''Syrian Arab Republic''
	WHEN ADDRESS_COMBINED LIKE ''% TW'' THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''% TJ'' THEN ''Tajikistan''
	WHEN ADDRESS_COMBINED LIKE ''% TZ'' THEN ''Tanzania, United Republic of''
	WHEN ADDRESS_COMBINED LIKE ''% TH'' THEN ''Thailand''
	WHEN ADDRESS_COMBINED LIKE ''% TL'' THEN ''Timor-Leste''
	WHEN ADDRESS_COMBINED LIKE ''% TG'' THEN ''Togo''
	WHEN ADDRESS_COMBINED LIKE ''% TK'' THEN ''Tokelau''
	WHEN ADDRESS_COMBINED LIKE ''% TO'' THEN ''Tonga''
	WHEN ADDRESS_COMBINED LIKE ''% TT'' THEN ''Trinidad and Tobago''
	WHEN ADDRESS_COMBINED LIKE ''% TN'' THEN ''Tunisia''
	WHEN ADDRESS_COMBINED LIKE ''% TR'' THEN ''Turkey''
	WHEN ADDRESS_COMBINED LIKE ''% TM'' THEN ''Turkmenistan''
	WHEN ADDRESS_COMBINED LIKE ''% TC'' THEN ''Turks and Caicos Islands''
	WHEN ADDRESS_COMBINED LIKE ''% TV'' THEN ''Tuvalu''
	WHEN ADDRESS_COMBINED LIKE ''% UG'' THEN ''Uganda''
	WHEN ADDRESS_COMBINED LIKE ''% UA'' THEN ''Ukraine''
	WHEN ADDRESS_COMBINED LIKE ''% AE'' THEN ''United Arab Emirates''
	WHEN ADDRESS_COMBINED LIKE ''% GB'' THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''% US'' THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''% UM'' THEN ''United States Minor Outlying Islands''
	WHEN ADDRESS_COMBINED LIKE ''% UY'' THEN ''Uruguay''
	WHEN ADDRESS_COMBINED LIKE ''% UZ'' THEN ''Uzbekistan''
	WHEN ADDRESS_COMBINED LIKE ''% VU'' THEN ''Vanuatu''
	WHEN ADDRESS_COMBINED LIKE ''% VE'' THEN ''Venezuela''
	WHEN ADDRESS_COMBINED LIKE ''% VN'' THEN ''Viet Nam''
	WHEN ADDRESS_COMBINED LIKE ''% VG'' THEN ''Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''% VI'' THEN ''Virgin Islands (US)''
	WHEN ADDRESS_COMBINED LIKE ''% WF'' THEN ''Wallis and Futuna''
	WHEN ADDRESS_COMBINED LIKE ''% EH'' THEN ''Western Sahara''
	WHEN ADDRESS_COMBINED LIKE ''% YE'' THEN ''Yemen''
	WHEN ADDRESS_COMBINED LIKE ''% ZM'' THEN ''Zambia''
	WHEN ADDRESS_COMBINED LIKE ''% ZW'' THEN ''Zimbabwe''
	end as P13
	FROM FULL_TABLE
	)

SELECT PT1.[Transaction Number], PT1.'+@ACCT_NO+',
coalesce(p1,p2,p3,p4,p5,p6,p7,p8,p9,p10,p11,p12,p13)'+@Country+'
INTO '+@Control_Temp_Table+' 
FROM PT1 
JOIN PT2 ON PT1.[Transaction Number] = PT2.[Transaction Number]
JOIN PT3 ON PT2.[Transaction Number] = PT3.[Transaction Number]
JOIN PT4 ON PT3.[Transaction Number] = PT4.[Transaction Number]
JOIN PT5 ON PT4.[Transaction Number] = PT5.[Transaction Number]
JOIN PT6 ON PT5.[Transaction Number] = PT6.[Transaction Number]
JOIN PT7 ON PT6.[Transaction Number] = PT7.[Transaction Number]
JOIN PT8 ON PT7.[Transaction Number] = PT8.[Transaction Number]
JOIN PT9 ON PT8.[Transaction Number] = PT9.[Transaction Number]
JOIN PT10 ON PT9.[Transaction Number] = PT10.[Transaction Number]
JOIN PT11 ON PT10.[Transaction Number] = PT11.[Transaction Number]
JOIN PT12 ON PT11.[Transaction Number] = PT12.[Transaction Number]
JOIN PT13 ON PT12.[Transaction Number] = PT13.[Transaction Number]

'

EXEC (@Script)
END
