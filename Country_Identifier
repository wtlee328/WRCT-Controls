	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]RS[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Serbia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SC[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Seychelles''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SL[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Sierra Leone''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Singapore''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SX[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Sint Maarten''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Slovakia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Slovenia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SB[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Solomon Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Somalia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ZA[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''South Africa''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GS[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''South Georgia and the South Sandwich I''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SS[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''South Sudan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ES[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Spain''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]LK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Sri Lanka''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SD[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Sudan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Suriname''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SJ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Svalbard and Jan Mayen''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Sweden''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]CH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Switzerland''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]SY[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Syrian Arab Republic''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Taiwan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TJ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tajikistan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tanzania, United Republic of''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Thailand''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TL[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Timor-Leste''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Togo''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TK[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tokelau''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TO[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tonga''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TT[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Trinidad and Tobago''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TN[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tunisia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TR[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Turkey''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Turkmenistan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TC[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Turks and Caicos Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]TV[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Tuvalu''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]UG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Uganda''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]UA[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Ukraine''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]AE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''United Arab Emirates''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]GB[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Great Britain''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]US[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''USA''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]UM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''United States Minor Outlying Islands''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]UY[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Uruguay''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]UZ[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Uzbekistan''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]VU[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Vanuatu''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]VE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Venezuela''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]VN[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Viet Nam''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]VG[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Virgin Islands (British)''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]VI[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Virgin Islands (US)''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]WF[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Wallis and Futuna''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]EH[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Western Sahara''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]YE[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Yemen''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ZM[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Zambia''
	WHEN ADDRESS_COMBINED LIKE ''[a-z][a-z][a-z][a-z]ZW[a-z0-9][a-z0-9]%'' AND (LEN(ADDRESS_COMBINED) = 11 OR LEN(ADDRESS_COMBINED) = 8) THEN ''Zimbabwe''
	end as P1
	FROM FULL_TABLE
	),

	PT2 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE 
	--Pattern 2: BIC Code 4+2+2=8digits
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Afghanistan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AX[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Aland Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Albania''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]DZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Algeria''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''American Samoa''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AD[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Andorra''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Angola''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AI[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Anguilla''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AQ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Antarctica''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Antigua and Barbuda''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Argentina''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Armenia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Aruba''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AU[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Australia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AT[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Austria''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]AZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Azerbaijan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BS[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bahamas''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bahrain''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BD[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bangladesh''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BB[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Barbados''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BY[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Belarus''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BE[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Belgium''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BZ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Belize''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BJ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Benin''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bermuda''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BT[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bhutan''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bolivia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BQ[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bonaire, Sint Eustatius and Saba''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bosnia and Herzegovina''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BW[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Botswana''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BV[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bouvet Island''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BR[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Brazil''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]IO[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''British Indian Ocean Territory''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Brunei Darussalam''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BG[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Bulgaria''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Burkina Faso''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]BI[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Burundi''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CV[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Cabo Verde''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]KH[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Cambodia''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CM[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Cameroon''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CA[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Canada''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]KY[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Cayman Islands''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CF[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Central African Republic''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]TD[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Chad''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CL[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Chile''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CN[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''China''
	When [PARTY_NAME] LIKE ''[a-z][a-z][a-z][a-z]CX[a-z0-9][a-z0-9]%'' AND (LEN(PARTY_NAME) = 11 OR LEN(PARTY_NAME) = 8) THEN ''Christmas Island''
