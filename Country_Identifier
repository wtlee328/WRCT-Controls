	WHEN ADDRESS_COMBINED LIKE ''%ZIMBABWE'' THEN ''Zimbabwe''
	end as P12
	FROM FULL_TABLE
	),

	PT13 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 14: Country with wildcard on either side; located anywhere in the address
	WHEN ADDRESS_COMBINED LIKE ''%Alabama%'' then ''USA''
