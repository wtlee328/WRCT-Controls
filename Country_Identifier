	WHEN ADDRESS_COMBINED LIKE ''%,ZW'' THEN ''Zimbabwe''
	end as P7
	FROM FULL_TABLE
	),

	PT8 AS(
	SELECT [Transaction Number],ADDRESS_COMBINED, CASE
	--Pattern 9: Country code with "/" and wildcard only on left side (at end of address)
	WHEN ADDRESS_COMBINED LIKE ''%/AF'' THEN ''Afghanistan''
	WHEN ADDRESS_COMBINED LIKE ''%/AX'' THEN ''Aland Islands''
